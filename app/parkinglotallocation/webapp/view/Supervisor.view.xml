<mvc:View
    xmlns:smarttable="sap.ui.comp.smarttable"
    controllerName="com.app.parkinglotallocation.controller.Supervisor"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:layout="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    displayBlock="true"
    height="100%"
>
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <tnt:ToolHeader id="IDGenToolHeader1">
                <Image
                    id="IDGenImage1"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://www.sap.com/dam/application/shared/logos/sap-logo-svg.svg"
                />
                <Button
                    id="sideNavigationToggleButton"
                    icon="sap-icon://menu"
                    type="Transparent"
                    press=".onSideNavButtonPress"
                >
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="IDGenOverflowToolbarLayoutData1"
                            priority="NeverOverflow"
                        />
                    </layoutData>
                </Button>
                <ToolbarSpacer
                    id="IDGenToolbarSpacer1"
                    width="20px"
                />

                <!-- Supervisor Information Section in Header -->
                <m:HBox
                    id="IDGenHBox1"
                    fitContainer="true"
                    alignItems="Center"
                >
                    <m:Avatar
                        id="IDGenAvatar1"
                        displaySize="S"
                        src="./Images/img(3).jpeg"
                        class="sapUiSmallMarginEnd"
                    />
                    <m:VBox id="IDGenVBox1">
                        <layout:HorizontalLayout id="_IDGenHorizontalLayout1">
                            <m:Text
                                id="IDGenText1"
                                text="{supervisor>/name}"
                                class="HeaderName"
                            />
                        </layout:HorizontalLayout>
                        <layout:HorizontalLayout id="_IDGenHorizontalLayout2">
                            <m:Link
                                id="IDGenLink1"
                                text="{supervisor>/phone}"
                            />
                        </layout:HorizontalLayout>
                        <!-- <layout:HorizontalLayout>
                            <m:Text text="{supervisor>/position}" />
                        </layout:HorizontalLayout> -->
                    </m:VBox>
                    <!-- <m:VBox>                       
                        <layout:HorizontalLayout>
                            <m:Label text="Email:" />
                            <m:Link text="{supervisor>/contact}" />
                        </layout:HorizontalLayout>
                    </m:VBox> -->
                </m:HBox>

                <ToolbarSpacer id="IDGenToolbarSpacer2" />

                <Button
                    id="IDGenButton1"
                    type="Transparent"
                    tooltip="Visualization"
                    icon="sap-icon://geographic-bubble-chart"
                    press="onGeographicBtnPress"
                />
                <Button
                    id="IDGenButton2"
                    icon="sap-icon://log"
                    type="Transparent"
                    press="onLogoutBtnPress"
                />
            </tnt:ToolHeader>
        </tnt:header>
        <!-- LeftSide Pannel Section -->
        <tnt:sideContent>
            <tnt:SideNavigation
                id="IDGenSideNavigation1"
                expanded="true"
                itemSelect=".onItemSelect"
            >
                <tnt:NavigationList id="idnavigation">
                    <tnt:NavigationListItem
                        id="idGenSlotAssign"
                        key="root1"
                        text="Slot Assignment:"
                        icon="sap-icon://activity-assigned-to-goal"
                    />
                    <tnt:NavigationListItem
                        id="idGenAllSlots"
                        key="root2"
                        text="All Slots:"
                        icon="sap-icon://dimension"
                    />
                    <tnt:NavigationListItem
                        id="idGenAllocated"
                        key="root3"
                        text="Allocated Slots:"
                        icon="sap-icon://shipping-status"
                    />
                    <tnt:NavigationListItem
                        id="idGenAvailable"
                        key="root4"
                        text="Available Slots:"
                        icon="sap-icon://check-availability"
                    />
                    <tnt:NavigationListItem
                        id="idGenHistory"
                        key="root5"
                        text="History:"
                        icon="sap-icon://customer-history"
                    />
                </tnt:NavigationList>
                <!-- <tnt:fixedItem>
					<tnt:NavigationList id="_IDGenNavigationList2" items="{path: '/fixedNavigation'}">
						<tnt:NavigationListItem id="_IDGenNavigationListItem3" text="{title}" icon="{icon}" />
					</tnt:NavigationList>
				</tnt:fixedItem> -->
            </tnt:SideNavigation>
        </tnt:sideContent>

        <!-- RightSide Pannel 1st Assign a slot... -->
        <tnt:mainContents>
            <NavContainer
                id="pageContainer"
                initialPage="page2"
            >
                <pages>
                    <ScrollContainer
                        id="root1"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <VBox
                            id="IDGenVBox2"
                            class="sapUiSmallMargin"
                        >
                            <form:SimpleForm
                                id="SimpleFormToolbar"
                                editable="true"
                                layout="ResponsiveGridLayout"
                                labelSpanXL="4"
                                labelSpanL="3"
                                labelSpanM="4"
                                labelSpanS="12"
                                adjustLabelSpan="false"
                                emptySpanXL="0"
                                emptySpanL="4"
                                emptySpanM="0"
                                emptySpanS="0"
                                columnsXL="2"
                                columnsL="1"
                                columnsM="1"
                                singleContainerFullSize="false"
                                ariaLabelledBy="Title1"
                            >
                                <form:toolbar>
                                    <Toolbar id="TB1">
                                        <Title
                                            id="Title1"
                                            text="SLOT AASIGNMENT:"
                                        />
                                        <ToolbarSpacer
                                            id="IDGenToolbarSpacer3"
                                        />
                                        <Button
                                            id="IDGenButton3"
                                            icon="sap-icon://action-settings"
                                        />
                                    </Toolbar>
                                </form:toolbar>
                                <form:content>
                                    <Toolbar
                                        id="IDGenToolbar1"
                                        ariaLabelledBy="Title2"
                                    >
                                        <Title
                                            id="Title2"
                                            text="Parking Slot Assign to Vehicle:"
                                        />
                                        <ToolbarSpacer
                                            id="IDGenToolbarSpacer4"
                                        />
                                    </Toolbar>
                                    <Label
                                        id="IDGenLabel1"
                                        text="Slot Number"
                                    />
                                    
                                    <m:Select 
                                        id="slotNumber"
                                        items="{path: '/AvailableSlots'}"
                                        width="80%"
                                        selectedKey="{assignmentModel>/slot/slotNumber}"
                                    >
                                        <core:Item
                                            id="IDGenItem1"
                                            key="{availSlot/slotNumber}"
                                            text="{availSlot/slotNumber}"
                                        />
                                    </m:Select >
                                    <Label
                                        id="IDGenLabel2"
                                        text="Driver Name"
                                    />
                                    <Input
                                        id="driverName"
                                        value="{assignmentModel>/driverName}"
                                        width="80%"
                                        placeholder="enter Driver name.."
                                    />
                                    <Label
                                        id="IDGenLabel3"
                                        text="Driver Number"
                                    />
                                    <Input
                                        id="driverNumber"
                                        value="{assignmentModel>/driverNumber}"
                                        width="80%"
                                        placeholder="enter Driver phone number.."
                                    />
                                    <Label
                                        id="IDGenLabel4"
                                        text="Vehicle Number"
                                    />
                                    <Input
                                        id="vehicleNumber"
                                        value="{assignmentModel>/vehicleNumber}"
                                        width="80%"
                                        placeholder="enter Vehicle Number.."
                                    />
                                    <Label
                                        id="IDGenLabel5"
                                        text="Vehicle Type"
                                    />
                                    <Input
                                        id="vehicleType"
                                        placeholder="enter Vehicle Type.."
                                        value="{assignmentModel>/vehicleType}"
                                    />
                                    <m:HBox
                                        id="id"
                                        class="RadioButtons"
                                        
                                    >
                                        <m:RadioButton
                                            id="idradio1"
                                            text="Inward"
                                            class="InwardBtn"
                                            valueState="Success"
                                            selected="{= ${assignmentModel>/serviceType} === 'inward'}"
                                            select=".onServiceTypeChange"
                                        />
                                        <m:RadioButton
                                            id="idradio1234"
                                            text="Outward"
                                            valueState="Success"
                                            selected="{= ${assignmentModel>/serviceType} === 'outward'}"
                                            select="onServiceTypeChange"
                                        />
                                    </m:HBox>

                                    <HBox
                                        id="IDGenHBox2"
                                        justifyContent="End"
                                    >
                                        <Button
                                            id="IDGenButton5"
                                            text="Assign"
                                            press="onAssignSlotPress"
                                            class="AssignClearSpace"
                                        />
                                        <Button
                                            id="IDGenButton6"
                                            text="Clear"
                                            press="onClearPress"
                                        />
                                    </HBox>
                                </form:content>
                            </form:SimpleForm>
                        </VBox>
                    </ScrollContainer>

                    <!-- RightSide Pannel 2nd All Slots... -->
                    <ScrollContainer
                        id="root2"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <m:VBox id="idVBoxallslots">
                            <form:SimpleForm
                                id="secondSimpleFormToolbar"
                                editable="true"
                                layout="ResponsiveGridLayout"
                                labelSpanXL="4"
                                labelSpanL="3"
                                labelSpanM="4"
                                labelSpanS="12"
                                adjustLabelSpan="false"
                                emptySpanXL="0"
                                emptySpanL="4"
                                emptySpanM="0"
                                emptySpanS="0"
                                columnsXL="2"
                                columnsL="1"
                                columnsM="1"
                                singleContainerFullSize="false"
                                ariaLabelledBy="Title1"
                            >
                                <form:toolbar>
                                    <Toolbar id="TB2">
                                        <Title
                                            id="Title1455"
                                            text="All SLOTS:"
                                        />
                                        <ToolbarSpacer
                                            id="IDGenToolbarSpacer345"
                                        />
                                    </Toolbar>
                                </form:toolbar>
                                <form:content>
                                    <Toolbar
                                        id="IDGenToolbar2"
                                        ariaLabelledBy="Title2"
                                    >
                                        <Title
                                            id="Title3"
                                            text="All Slots from the Parking Space:"
                                        />
                                    </Toolbar>
                                    <Table
                                        id="allSlotsTable"
                                        width="auto"
                                        items="{/AllSlots}"
                                    >
                                        <columns>
                                            <Column id="IDGenColumn1">
                                                <Text
                                                    id="IDGenText5"
                                                    text="Slot Number:"
                                                />
                                            </Column>
                                            <Column id="IDGenColumn2">
                                                <Text
                                                    id="IDGenText6"
                                                    text="Service Type:"
                                                />
                                            </Column>
                                            <Column id="IDGenColumn3">
                                                <Text
                                                    id="IDGenText7"
                                                    text="Status: (Available/Occupaid)"
                                                />
                                            </Column>
                                        </columns>
                                        <items>
                                            <ColumnListItem
                                                id="IDGenColumnListItem1"
                                            >
                                                <cells>
                                                    <Text
                                                        id="IDGenText8"
                                                        text="{slotNumber}"
                                                    />
                                                    <Text
                                                        id="IDGenText9"
                                                        text="{serviceType}"
                                                    />
                                                    <Text
                                                        id="IDGenText10"
                                                        text="{status}"
                                                    />
                                                </cells>
                                            </ColumnListItem>
                                        </items>
                                    </Table>
                                </form:content>
                            </form:SimpleForm>
                        </m:VBox>
                    </ScrollContainer>

                    <!-- RightSide Pannel 3rd Assigned slots... -->
                    <ScrollContainer
                        id="root3"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <m:VBox id="idVBoxallocatedslots">
                            <form:SimpleForm
                                id="thirdSimpleFormToolbar"
                                editable="true"
                                layout="ResponsiveGridLayout"
                                labelSpanXL="4"
                                labelSpanL="3"
                                labelSpanM="4"
                                labelSpanS="12"
                                adjustLabelSpan="false"
                                emptySpanXL="0"
                                emptySpanL="4"
                                emptySpanM="0"
                                emptySpanS="0"
                                columnsXL="2"
                                columnsL="1"
                                columnsM="1"
                                singleContainerFullSize="false"
                                ariaLabelledBy="Title1"
                            >
                                <form:toolbar>
                                    <Toolbar id="TB3">
                                        <Title
                                            id="TitleAllocated"
                                            text="ASSIGNED SLOTS:"
                                        />
                                        <ToolbarSpacer
                                            id="IDGenToolbarSpacerallocated"
                                        />
                                        <Button
                                            id="editButton"
                                            text="Edit"
                                            type="Transparent"
                                            press="onEdit"
                                        />
                                        <Button
                                            id="saveButton"
                                            text="Save"
                                            type="Transparent"
                                            press="onSave"
                                            visible="false"
                                        />
                                        <Button
                                            id="cancelButton"
                                            text="Cancel"
                                            type="Transparent"
                                            press="onCancel"
                                            visible="false"
                                        />
                                    </Toolbar>
                                </form:toolbar>
                                <form:content>
                                    <Toolbar
                                        id="IDGenToolbarallocated2"
                                        ariaLabelledBy="Title2"
                                    >
                                        <Title
                                            id="Title4"
                                            text="Assigned Slots from the Parking Space:"
                                        />
                                    </Toolbar>
                                    <Table
                                        id="AllocatedSlotsTable"
                                        width="auto"
                                        items="{/AllocatedSlots}"
                                    >
                                        <columns>
                                            <Column id="Allocatedcol1">
                                                <Text
                                                    id="IDGenAllocatedText1"
                                                    text="Slot Number:"
                                                />
                                            </Column>
                                            <Column id="Allocatedcol7">
                                                <Text
                                                    id="IDGenAllocatedText7"
                                                    text="Vehicle Type:"
                                                />
                                            </Column>
                                            <Column id="Allocatedcol2">
                                                <Text
                                                    id="IDGenAllocatedTextt2"
                                                    text="Vehicle Number:"
                                                />
                                            </Column>
                                            <Column id="Allocatedcol3">
                                                <Text
                                                    id="IDGenAllocatedText3"
                                                    text="Driver Number:"
                                                />
                                            </Column>
                                            <Column id="Allocatedcol4">
                                                <Text
                                                    id="IDGenAllocatedText4"
                                                    text="Driver Name:"
                                                />
                                            </Column>
                                            <Column id="Allocatedcol8">
                                                <Text
                                                    id="IDGenAllocatedText8"
                                                    text="Service Type:"
                                                />
                                            </Column>
                                            <Column id="Allocatedcol5">
                                                <Text
                                                    id="IDGenAllocatedText5"
                                                    text="In Time:"
                                                />
                                            </Column>
                                            <!-- <Column id="Allocatedcol6">
                                                <Text
                                                    id="IDGenAllocatedText6"
                                                    text="Out Time:"
                                                />
                                            </Column> -->
                                        </columns>
                                        <items>
                                            <ColumnListItem
                                                id="IDGenAllocatedColListItems"
                                            >
                                                <cells>
                                                    <Text
                                                        id="IDGenText01"
                                                        text="{slot/slotNumber}"
                                                    />
                                                    <Text
                                                        id="IDGenText07"
                                                        text="{vehType}"
                                                    />
                                                    <Text
                                                        id="IDGenText02"
                                                        text="{vehNumber}"
                                                    />
                                                    <Text
                                                        id="IDGenText03"
                                                        text="{driverNumber}"
                                                    />
                                                    <Text
                                                        id="IDGenText04"
                                                        text="{driverName}"
                                                    />
                                                    <Text
                                                        id="IDGenText08"
                                                        text="{slot/serviceType}"
                                                    />
                                                    <Text
                                                        id="IDGenText05"
                                                        text="{inTime}"
                                                    />
                                                    <!-- <Text
                                                        id="IDGenText06"
                                                        text="{outTime}"
                                                    /> -->
                                                </cells>
                                            </ColumnListItem>
                                        </items>
                                    </Table>
                                </form:content>
                            </form:SimpleForm>
                        </m:VBox>
                    </ScrollContainer>

                    <!-- RightSide Pannel 4th Available Slots... -->
                    <ScrollContainer
                        id="root4"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <m:VBox id="idVBoxAvailslots">
                            <form:SimpleForm
                                id="fourthSimpleFormToolbar"
                                editable="true"
                                layout="ResponsiveGridLayout"
                                labelSpanXL="4"
                                labelSpanL="3"
                                labelSpanM="4"
                                labelSpanS="12"
                                adjustLabelSpan="false"
                                emptySpanXL="0"
                                emptySpanL="4"
                                emptySpanM="0"
                                emptySpanS="0"
                                columnsXL="2"
                                columnsL="1"
                                columnsM="1"
                                singleContainerFullSize="false"
                                ariaLabelledBy="Title1"
                            >
                                <form:toolbar>
                                    <Toolbar id="TB4">
                                        <Title
                                            id="Title1455896"
                                            text="AVAILABLE SLOTS:"
                                        />
                                        <ToolbarSpacer id="IDGenToolbar345" />
                                    </Toolbar>
                                </form:toolbar>
                                <form:content>
                                    <Toolbar
                                        id="IDGenToolbar4"
                                        ariaLabelledBy="Title2"
                                    >
                                        <Title
                                            id="Title5"
                                            text="Available Slots Only from the Space:"
                                        />
                                    </Toolbar>
                                    <Table
                                        id="AvailSlotsTable"
                                        width="auto"
                                        items="{/AvailableSlots}"
                                    >
                                        <columns>
                                            <Column id="IDGenColumn001">
                                                <Text
                                                    id="IDGenText001"
                                                    text="Slot Number:"
                                                />
                                            </Column>
                                            <Column id="IDGenColumn002">
                                                <Text
                                                    id="IDGenText002"
                                                    text="Service Type:"
                                                />
                                            </Column>
                                        </columns>
                                        <items>
                                            <ColumnListItem
                                                id="IDGenAvailColumnListItem1"
                                            >
                                                <cells>
                                                    <Text
                                                        id="IDGenAvailText8"
                                                        text="{availSlot/slotNumber}"
                                                    />
                                                    <Text
                                                        id="IDGenAvailText9"
                                                        text="{availSlot/serviceType}"
                                                    />
                                                </cells>
                                            </ColumnListItem>
                                        </items>
                                    </Table>
                                </form:content>
                            </form:SimpleForm>
                        </m:VBox>
                    </ScrollContainer>

                    <!-- RightSide Pannel 5th History... -->
                    <m:ScrollContainer
                        id="root5"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <m:VBox id="idVBoxHistory">
                            <form:SimpleForm
                                id="fifthSimpleFormToolbar"
                                editable="true"
                                layout="ResponsiveGridLayout"
                                labelSpanXL="4"
                                labelSpanL="3"
                                labelSpanM="4"
                                labelSpanS="12"
                                adjustLabelSpan="false"
                                emptySpanXL="0"
                                emptySpanL="4"
                                emptySpanM="0"
                                emptySpanS="0"
                                columnsXL="2"
                                columnsL="1"
                                columnsM="1"
                                singleContainerFullSize="false"
                                ariaLabelledBy="Title1"
                            >
                                <form:toolbar>
                                    <Toolbar id="TB5">
                                        <Title
                                            id="Title145589676"
                                            text="TOATL HISTORY:"
                                        />
                                        <ToolbarSpacer id="IDGenToolbar34589" />
                                        <m:Input
                                            id="idInputSearch"
                                            liveChange="onLiveSearchAnything"
                                            width="30%"
                                            placeholder="Search Anything..."
                                        />
                                        <!-- <Button
                                            id="IDGenSearch"
                                            icon="sap-icon://search"
                                            type="Transparent"
                                            press="onSearchBtnPress"
                                        /> -->
                                    </Toolbar>
                                </form:toolbar>
                                <form:content>
                                    <Table
                                        id="idHistoryTable"
                                        width="auto"
                                    >
                                        <columns>
                                            <Column id="IDGenColumn0011">
                                                <Text
                                                    id="IDGenText0011"
                                                    text="Slot Number:"
                                                />
                                            </Column>
                                            <Column id="IDGenColumn0022">
                                                <Text
                                                    id="IDGenText0022"
                                                    text="Service Type:"
                                                />
                                            </Column>
                                            <Column id="IDGenColumn0033">
                                                <Text
                                                    id="IDGenText0033"
                                                    text="Vehicle Number:"
                                                />
                                            </Column>
                                            <Column id="IDGenColumn0044">
                                                <Text
                                                    id="IDGenText0044"
                                                    text="Driver Number:"
                                                />
                                            </Column>
                                            <Column id="IDGenColumn0055">
                                                <Text
                                                    id="IDGenText0055"
                                                    text="Driver Name:"
                                                />
                                            </Column>
                                            <Column id="IDGenColumn0066">
                                                <Text
                                                    id="IDGenText0066"
                                                    text="In Time:"
                                                />
                                            </Column>
                                            <Column id="IDGenColumn0077">
                                                <Text
                                                    id="IDGenText0077"
                                                    text="Out Time:"
                                                />
                                            </Column>
                                        </columns>
                                        <items>
                                            <ColumnListItem
                                                id="IDGenHistoryColumnListItem1"
                                            >
                                                <cells>
                                                    <Text
                                                        id="IDGenHistoryText8"
                                                        text=""
                                                    />
                                                    <Text
                                                        id="IDGenHistoryText9"
                                                        text=""
                                                    />
                                                    <Text
                                                        id="IDGenHistoryText10"
                                                        text=""
                                                    />
                                                    <Text
                                                        id="IDGenHistoryText11"
                                                        text=""
                                                    />
                                                    <Text
                                                        id="IDGenHistoryText12"
                                                        text=""
                                                    />
                                                    <Text
                                                        id="IDGenHistoryText13"
                                                        text=""
                                                    />
                                                    <Text
                                                        id="IDGenHistoryText14"
                                                        text=""
                                                    />
                                                </cells>
                                            </ColumnListItem>
                                        </items>
                                    </Table>
                                </form:content>
                            </form:SimpleForm>
                        </m:VBox>
                    </m:ScrollContainer>
                    <ScrollContainer
                        id="page2"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <m:Image
                            id="IdImageright"
                            src="./Images/image2.jpeg"
                            width="100%"
                            height="auto"
                        />
                    </ScrollContainer>
                </pages>
            </NavContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>